    "eslint-plugin-jest-dom": "^3.8.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "jest-environment-node": "^26.6.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@stablelib/base64": "^1.0.0",
